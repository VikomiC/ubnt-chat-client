.dialogContainer {
    position: absolute;
    width: 400px;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    border: 0;
    background-color: #fff;
    box-shadow:
        0 11px 15px -7px rgba(0,0,0,.2),
        0 24px 38px 3px rgba(0,0,0,.14),
        0 9px 46px 8px rgba(0,0,0,.12);
    padding: 32px;
    outline: none;
}
